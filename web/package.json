{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --fix './**/*.{ts,tsx}'",
    "lint:check": "eslint './**/*.{ts,tsx}'",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"./**/*.{ts,tsx}\"",
    "tsc": "tsc -b",
    "tsc:check": "tsc -b --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
