{
  "plugins": ["react", "jsx-a11y", "import", "unicorn", "promise"],
  "categories": {
    "correctness": "error",
    "suspicious": "error",
    "pedantic": "warn",
    "style": "warn"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn",
    "no-debugger": "error",
    "no-empty": "error",
    "no-extra-boolean-cast": "error",
    "no-irregular-whitespace": "error",
    "no-unsafe-finally": "error",
    "no-unsafe-negation": "error",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": "error",
    "no-implicit-coercion": "warn",
    "curly": "error"
  },
  "env": {
    "browser": true,
    "es2020": true
  },
  "globals": {
    "React": "readonly"
  },
  "ignore": [
    "dist/**",
    "src/routeTree.gen.ts",
    "src/routeTree.gen.d.ts",
    "coverage/**",
    "node_modules/**"
  ]
}
